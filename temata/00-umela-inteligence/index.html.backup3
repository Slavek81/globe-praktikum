    <script src="../../js/main.js"></script>
    <script>
        // Kv√≠z funkce
        function checkAnswer(questionId, selectedAnswer, isCorrect) {
            const resultElement = document.getElementById(questionId + '-result');
            const options = document.querySelectorAll(`#${questionId.replace('q', '')} .quiz-option`);

            // Reset v≈°ech mo≈ænost√≠
            options.forEach(option => {
                option.classList.remove('correct', 'incorrect');
            });

            // Oznaƒçit vybranou odpovƒõƒè
            event.target.classList.add(isCorrect ? 'correct' : 'incorrect');

            if (isCorrect) {
                resultElement.innerHTML = '<span style="color: #16a34a;"><i class="fas fa-check"></i> Spr√°vnƒõ!</span>';
                resultElement.style.background = '#dcfce7';
            } else {
                resultElement.innerHTML = '<span style="color: #dc2626;"><i class="fas fa-times"></i> Zkus to znovu.</span>';
                resultElement.style.background = '#fef2f2';
            }
        }

        // Etika accordion
        function toggleEthics(id) {
            const content = document.getElementById(id);
            const header = content.previousElementSibling;
            const icon = header.querySelector('i');

            content.classList.toggle('active');
            icon.classList.toggle('fa-chevron-down');
            icon.classList.toggle('fa-chevron-up');
        }

        // Demo generov√°n√≠ b√°snƒõ s anal√Ωzou promptu
        function generatePoem() {
            const input = document.getElementById('prompt-input').value;
            const output = document.getElementById('poem-output');
            const analysisDiv = document.getElementById('prompt-analysis');
            const analysisContent = document.getElementById('analysis-content');

            if (!input.trim()) {
                output.innerHTML = '<em style="color: #dc2626;">Nejd≈ô√≠v zadejte prompt!</em>';
                analysisDiv.style.display = 'none';
                return;
            }

            // Anal√Ωza promptu
            const analysis = analyzePrompt(input);

            // Simulace AI odpovƒõdi (v re√°ln√© aplikaci by se volalo API)
            const poems = [
                "Gravitace t√°hne dol≈Ø,<br>Icaro letƒõl moc vysoko.<br>Newton pod stromekem sedƒõl,<br>Jablko mu na hlavu kleslo!",
                "F = ma, to je z√°kon,<br>Newton n√°m ho napsal v knih√°ch.<br>S√≠la, hmota, zrychlen√≠,<br>Fyzika je √∫≈æasn√°!",
                "Einstein ≈ôekl E=mc¬≤,<br>Hmota je energie v ko≈ôeni.<br>ƒåas se krout√≠, prostor taky,<br>Relativita - to jsou fajn z√°kony!"
            ];

            const randomPoem = poems[Math.floor(Math.random() * poems.length)];

            output.innerHTML = `
                <strong>Vygenerovan√° b√°se≈à:</strong><br><br>
                <em>${randomPoem}</em>
                <br><br>
                <small><i class="fas fa-lightbulb"></i> V re√°ln√© aplikaci by tuto b√°se≈à vygenerovala AI na z√°kladƒõ va≈°eho promptu.</small>
            `;

            // Zobrazen√≠ anal√Ωzy
            analysisContent.innerHTML = `
                <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 1rem;">
                    <div>
                        <strong>üìè D√©lka:</strong><br>
                        ${analysis.wordCount} slov, ${analysis.tokenEstimate} token≈Ø (odhad)
                    </div>
                    <div>
                        <strong>üéØ Konkr√©tnost:</strong><br>
                        <span style="color: ${analysis.specificity.color};">${analysis.specificity.level}</span>
                    </div>
                    <div>
                        <strong>üë§ Role definov√°na:</strong><br>
                        <span style="color: ${analysis.hasRole ? '#16a34a' : '#dc2626'};">${analysis.hasRole ? 'Ano' : 'Ne'}</span>
                    </div>
                    <div>
                        <strong>üìã Form√°t zad√°n:</strong><br>
                        <span style="color: ${analysis.hasFormat ? '#16a34a' : '#dc2626'};">${analysis.hasFormat ? 'Ano' : 'Ne'}</span>
                    </div>
                </div>
                <div style="margin-top: 1rem;">
                    <strong>üí° Doporuƒçen√≠:</strong> ${analysis.suggestions.join(', ')}
                </div>
            `;

            analysisDiv.style.display = 'block';
        }

        // Funkce pro anal√Ωzu promptu
        function analyzePrompt(prompt) {
            const words = prompt.trim().split(/\s+/);
            const wordCount = words.length;
            const tokenEstimate = Math.ceil(wordCount * 1.3); // Hrub√Ω odhad

            // Kontrola konkr√©tnosti
            const specificWords = ['konkr√©tn√≠', 'detailn√≠', 'p≈ôesn√Ω', 'specifick√Ω', 'pro', 'vƒõk', '√∫rove≈à', '≈°kolu'];
            const hasSpecificWords = specificWords.some(word => prompt.toLowerCase().includes(word));

            let specificity = { level: 'N√≠zk√°', color: '#dc2626' };
            if (wordCount > 15 && hasSpecificWords) {
                specificity = { level: 'Vysok√°', color: '#16a34a' };
            } else if (wordCount > 10 || hasSpecificWords) {
                specificity = { level: 'St≈ôedn√≠', color: '#f59e0b' };
            }

            // Kontrola role
            const roleWords = ['jsi', 'jste', 'jako', 'expert', 'uƒçitel', 'student', 'b√°sn√≠k', 'spisovatel'];
            const hasRole = roleWords.some(word => prompt.toLowerCase().includes(word));

            // Kontrola form√°tu
            const formatWords = ['ver≈°', '≈ô√°dek', 'bod', 'seznam', 'kr√°tk√Ω', 'dlouh√Ω', 'str√°nka', 'odstavec', 'r√Ωm'];
            const hasFormat = formatWords.some(word => prompt.toLowerCase().includes(word));

            // Doporuƒçen√≠
            const suggestions = [];
            if (!hasRole) suggestions.push('Definujte roli AI (nap≈ô. "Jsi b√°sn√≠k...")');
            if (!hasFormat) suggestions.push('Specifikujte form√°t v√Ωstupu');
            if (wordCount < 10) suggestions.push('Buƒète konkr√©tnƒõj≈°√≠ v zad√°n√≠');
            if (suggestions.length === 0) suggestions.push('V√Ωborn√Ω prompt! üëè');

            return {
                wordCount,
                tokenEstimate,
                specificity,
                hasRole,
                hasFormat,
                suggestions
            };
        }

        // Prompt Builder funkce
        function buildPrompt() {
            const role = document.getElementById('role-select').value;
            const topic = document.getElementById('topic-select').value;
            const format = document.getElementById('format-select').value;
            const audience = document.getElementById('audience-select').value;

            if (!role || !topic || !format || !audience) {
                alert('Pros√≠m vyberte v≈°echny mo≈ænosti.');
                return;
            }

            const prompt = `${role}. Vysvƒõtli ${topic} ${audience} ${format}. Pou≈æij jednoduch√© p≈ô√≠klady a zaj√≠mav√© analogie.`;

            document.getElementById('prompt-result').textContent = prompt;
            document.getElementById('built-prompt').style.display = 'block';
        }

        function copyPrompt() {
            const promptText = document.getElementById('prompt-result').textContent;
            navigator.clipboard.writeText(promptText).then(function() {
                const button = event.target;
                const originalText = button.textContent;
                button.textContent = 'Zkop√≠rov√°no! ‚úì';
                button.style.background = '#16a34a';

                setTimeout(() => {
                    button.textContent = originalText;
                    button.style.background = 'var(--secondary-color)';
                }, 2000);
            }).catch(function(err) {
                console.error('Chyba p≈ôi kop√≠rov√°n√≠: ', err);
                alert('Kop√≠rov√°n√≠ se nezda≈ôilo. Zkuste text vybrat a zkop√≠rovat ruƒçnƒõ.');
            });
        }
    </script>
